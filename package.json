{
  "name": "obsidian-learning-path-generator",
  "version": "0.7.6",
  "description": "Generate learning paths and curriculum from your vault notes with AI",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && cp src/styles.css styles.css",
    "build:css": "cp src/styles.css styles.css",
    "typecheck": "tsc -noEmit -skipLibCheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "ai",
    "learning",
    "pkm",
    "spaced-repetition",
    "curriculum"
  ],
  "author": "eohjun",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.27.2",
    "jest": "^29.7.0",
    "obsidian": "^1.11.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.9.3"
  }
}
